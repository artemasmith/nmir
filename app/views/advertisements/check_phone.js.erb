<% if @search_results.present? %>
    $('.form-group.DuplicateResults').html('<%= j (render('advertisements/form_duplicate')) %>');
    $('.DuplicatePhones').removeClass('hidden').html("Найдено <%=@search_results.count%> объявлений с таким номером");
<% else %>
    $('.DuplicatePhones').addClass('hidden');
<% end %>
